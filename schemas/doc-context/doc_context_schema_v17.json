          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "text": {
          "$ref": "#/definitions/optionalString"
        },
        "title": {
          "type": "string"
        },
        "uuid": {
          "$ref": "#/definitions/uuid"
        },
        "annotations": {
          "$ref": "#/definitions/listDict"
        }
      },
      "additionalProperties": false
    },
    "kmExpert": {
