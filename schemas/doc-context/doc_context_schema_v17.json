          "properties": {
            "answers": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmAnswer" }
              }
            },
            "choices": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmChoice" }
              }
            },
            "chapters": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmChapter" }
              }
            },
            "experts": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmExpert" }
              }
            },
            "metrics": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmMetric" }
              }
            },
            "phases": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmPhase" }
              }
            },
            "integrations": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmIntegration" }
              }
            },
            "questions": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmQuestion" }
              }
            },
            "references": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmReference" }
              }
            },
            "tags": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmTag" }
              }
            },
            "resourceCollections": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmResourceCollection" }
              }
            },
            "resourcePages": {
              "type": "object",
              "patternProperties": {
                ".*": { "$ref": "#/definitions/kmResourcePage" }
              }
            }
          },
          "additionalProperties": false
        },
        "integrationUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "name": {
          "type": "string"
        },
        "tagUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "metricUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "phaseUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "resourceCollectionUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "uuid": {
          "$ref": "#/definitions/uuid"
        },
        "annotations": {
          "$ref": "#/definitions/listDict"
        }
      },
      "additionalProperties": false
    },
    "kmAnswer": {
      "type": "object",
      "title": "Answer",
      "properties": {
        "advice": {
          "$ref": "#/definitions/optionalString"
        },
        "followUpUuids": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/uuid"
          }
        },
        "label": {
          "type": "string"
        },
        "metricMeasures": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/metricMeasure"
          }
        },
        "uuid": {
          "$ref": "#/definitions/uuid"
        },
        "annotations": {
          "$ref": "#/definitions/listDict"
        }
      },
      "additionalProperties": false
    },
    "kmChapter": {
      "type": "object",
      "title": "Chapter",
      "properties": {
