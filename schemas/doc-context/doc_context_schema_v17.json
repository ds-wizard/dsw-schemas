        {
          "$ref": "#/definitions/kmApiIntegration"
        },
        {
          "$ref": "#/definitions/kmApiLegacyIntegration"
        },
        {
          "$ref": "#/definitions/kmWidgetIntegration"
        }
      ]
    },
    "kmValueQuestion": {
