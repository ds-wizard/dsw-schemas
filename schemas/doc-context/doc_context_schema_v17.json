        "allowCustomReply": {
          "type": "boolean"
        },
        "requestUrl": {
          "type": "string"
        },
        "requestBody": {
          "type": "string"
        },
        "requestHeaders": {
          "$ref": "#/definitions/listDict"
        },
        "requestMethod": {
          "type": "string"
        },
        "requestAllowEmptySearch": {
          "type": "boolean"
        },
        "responseListField": {
